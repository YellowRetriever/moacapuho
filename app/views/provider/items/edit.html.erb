<%= form_with model: [:provider,@item], local: true do |f| %>
    <div>
        <label>name</label>
        <%= f.text_field :name %>
    </div>
    <div>
        <label>status</label>
        <%= f.select :status, Item.statuses.keys.map{|k| [I18n.t("enums.item.status.#{k}"), k]} %>
    </div>
    <div>
        <label>prefecture</label>
        <%= f.text_field :prefecture %>
    </div>
    <div>
        <label>city</label>
        <%= f.text_field :city %>
    </div>
    <div>
        <label>address</label>
        <%= f.text_field :address %>
    </div>
    <div>
        <label>text</label>
        <%= f.text_field :text %>
    </div>
    <div>
        <%= f.submit "æ›´æ–°" %>
    </div>
    
<% end %>